# Change to one to enable stunnel automatic startup
ENABLED=<%= scope.lookupvar('stunnel::init_enabled') %>
FILES="<%= scope.lookupvar('stunnel::init_files') %>"
OPTIONS="<%= scope.lookupvar('stunnel::init_options') %>"

# Change to one to enable ppp restart scripts
PPP_RESTART=<%= scope.lookupvar('stunnel::init_ppp_restart') %>
